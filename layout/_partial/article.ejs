<div class="postBox">
    <article class="post">
      <div class="post-title">
        <div class="post-title">
          <a><%= page.title %></a>
        </div>
      </div>
      <div class="post-meta">
        <%- partial('_partial/post-meta',{post: page}) %>
        <span id="busuanzi_container_page_pv">
          / <span id="busuanzi_value_page_pv"></span> views
        </span>
      </div>
      <div class="post-content" id ="post-content">
        <%- page.content %>
      </div>
    </article>
    <%- partial('_partial/disqus') %>
</div>
